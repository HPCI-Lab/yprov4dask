@prefix dskp: <file://./dask-prov.ttl> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<file://./dask-prov.ttl> a owl:Ontology ;
	dc:title "The vocabulary for dask provenance tracker" ;
	dc:date "2025-05-19" ;
	dc:description "This is the RDF Schema for the vocabulary found in the provenance document generated by yprov4dask" .

dskp:name a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "name" ;
	rdfs:comment "The name of the function" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:module a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "module" ;
	rdfs:comment "The module in which the function has been defined" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:group a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "name" ;
	rdfs:comment "The group to which this function call belong w.r.t. the Dask scheduler view" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:name a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "name" ;
	rdfs:comment "The human readable name of the function as defined in source code" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:value a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "value" ;
	rdfs:comment "The value of the entity" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:dtype a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "dtype" ;
	rdfs:comment "The datatype of the value" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:nbyte a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "nbyte" ;
	rdfs:comment "The size in bytes of the value" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:is_error a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "is_error" ;
	rdfs:comment "Tells if this value is an exception raised because an error has occured" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:exception_text a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "exception_text" ;
	rdfs:comment "The text message of the exception" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:stacktrace a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "stacktrace" ;
	rdfs:comment "The stacktrace of the exception" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:blamed_task a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "blamed_task" ;
	rdfs:comment "Key for the subtask that generated this exception" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:as_parameter a rdf:Property ;
	rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "as_parameter" ;
	rdfs:comment "Name of the parameter of a function that used this entity" ;
	rdfs:range prov:used;
	rdfs:domain rdfs:Literal .