@prefix dskp: <file://./dask-prov.ttl> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<file://./dask-prov.ttl> a owl:Ontology ;
	dc:title "The vocabulary for dask provenance tracker" ;
	dc:date "2025-05-19" ;
	dc:description "This is the RDF Schema for the vocabulary found in the provenance document generated by yprov4dask" .

dskp:name a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "name" ;
	rdfs:comment "The name of the function" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:module a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "module" ;
	rdfs:comment "The module in which the function has been defined" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:group a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "name" ;
	rdfs:comment "The group to which this function call belong w.r.t. the Dask scheduler view" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:value a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "value" ;
	rdfs:comment "The value of the entity" ;
	rdfs:range prov:Entity ;
	rdfs:domain rdfs:Literal .

dskp:dtype a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "dtype" ;
	rdfs:comment "The datatype of the value" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .

dskp:nbyte a rdf:Propery ;
    rdfs:isDefinedBy <file://./dask-prov.ttl> ;
	rdfs:label "nbyte" ;
	rdfs:comment "The size in bytes of the value" ;
	rdfs:range prov:Activity ;
	rdfs:domain rdfs:Literal .
